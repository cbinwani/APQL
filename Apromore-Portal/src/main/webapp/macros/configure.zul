<window width="900px" closable="true" position="center" title="Configure process model with Quaestio">
    <applet id="quaestio"
         codebase="applets/"
         archive="quaestio-applet.jar"
         code="com.processconfiguration.quaestio.QuaestioApplet"
         mayscript="true"
         width="900px" height="600px"/>
    <script defer="true">
         function showModel(x,y) {
             alert('Model x=' + x + " y=" + y);
         }
    </script>
</window>
